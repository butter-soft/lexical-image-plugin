{
  "name": "@bvtter/lexical-image-plugin",
  "version": "0.0.1",
  "description": "Resizable Image node for Lexical.",
  "keywords": [
    "react",
    "lexical",
    "editor",
    "image"
  ],
  "license": "MIT",
  "contributors": [
    {
      "name": "Jung Wook Park",
      "email": "jwpark@buttersoft.io"
    }
  ],
  "type": "module",
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "lint": "oxlint"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "oxlint": "^0.16.9",
    "prettier": "^3.5.3",
    "tsdown": "^0.11.9",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@lexical/react": "^0.31.1",
    "@lexical/utils": "^0.31.1",
    "lexical": "^0.31.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "peerDependencies": {
    "lexical": "^0.31.1",
    "react": "^19.1.0"
